    "axios": "^0.19.2",
    "bignumber.js": "^9.0.0",
    "bip32": "^2.0.5",
    "bip39": "^3.0.2",
    "bitbox-sdk": "^8.11.2",
    "bitcoinjs-lib": "^5.1.10",
    "cardano-wallet": "^1.2.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
