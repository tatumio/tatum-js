{
  "root": "packages/blockchain/scrypta",
  "sourceRoot": "packages/blockchain/scrypta/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/blockchain/scrypta",
        "main": "packages/blockchain/scrypta/src/index.ts",
        "packageJson": "packages/blockchain/scrypta/package.json",
        "tsConfig": "packages/blockchain/scrypta/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/blockchain/scrypta/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/blockchain/scrypta"],
      "options": {
        "jestConfig": "packages/blockchain/scrypta/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:sdk", "scope:btc-based"]
}
