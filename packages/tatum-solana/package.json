{
  "name": "@tatumio/tatum-solana",
  "version": "2.0.0-alpha.79",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "test": "jest",
