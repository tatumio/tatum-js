{
  "name": "tatumio",
  "version": "2.0.0-alpha.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx affected:build",
    "build:libs": "nx run-many --target=build --projects=$(nx affected:libs --plain | sed 's/ /,/g')",
    "test:ci": "nx run-many --target=test --all --parallel --max-parallel=3 --coverage --coverageReporters=lcov && node ./tools/coverageMerger.js",
    "lint": "nx affected:lint --parallel",
    "format": "nx format:write",
    "test": "nx affected:test --parallel",
    "dep-graph": "nx dep-graph",
    "generate-api": "openapi --exportCore false --client axios --input ./packages/api-client/src/api_schema.json --output ./packages/api-client/src/generated",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@tatumio/bitcoincashjs2-lib": "^4.1.2",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.2",
    "bip32": "^2.0.5",
    "bip39": "^3.0.2",
    "bitcoinjs-lib": "^5.2.0",
    "bitcore-lib": "^8.25.25",
    "bitcore-lib-doge": "^8.25.27",
    "bitcore-lib-ltc": "^8.25.27",
    "cashaddrjs": "^0.4.4",
    "coininfo": "^5.1.0",
    "cross-blob": "^3.0.0",
    "ethereumjs-wallet": "^1.0.2",
    "form-data": "^4.0.0",
    "hdkey": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "ripple-lib": "^1.7.1",
    "tslib": "^2.0.0",
    "web3": "^1.7.0",
    "web3-core": "^1.7.0",
    "web3-eth": "^1.7.0",
    "web3-utils": "^1.7.0"
  },
  "devDependencies": {
    "@nrwl/cli": "13.5.3",
    "@nrwl/eslint-plugin-nx": "13.5.3",
    "@nrwl/jest": "13.5.3",
    "@nrwl/js": "13.5.3",
    "@nrwl/linter": "13.5.3",
    "@nrwl/node": "13.5.3",
    "@nrwl/nx-cloud": "13.1.2",
    "@nrwl/tao": "13.5.3",
    "@nrwl/workspace": "13.5.3",
    "@types/bignumber.js": "^5.0.0",
    "@types/bip32": "^2.0.0",
    "@types/bip39": "^3.0.0",
    "@types/bitcore-lib": "^0.15.3",
    "@types/cashaddrjs": "^0.3.0",
    "@types/hdkey": "^2.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.9.1",
    "@typescript-eslint/parser": "~5.9.1",
    "eslint": "8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "expect-more-jest": "^5.4.0",
    "faker": "^5.5.3",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "openapi-typescript-codegen": "^0.12.5",
    "prettier": "2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.4"
  }
}
