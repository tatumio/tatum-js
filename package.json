    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^3.0.3",
    "typescript": "^3.9.6"
  },
  "jest": {
