    "ts-node": "ts-node",
    "build": "tsc -p tsconfig.json",
    "build-all": "yarn clean && yarn build"
  },
  "dependencies": {
    "bignumber.js": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "tslib": "^2.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.14.0",
    "jest": "27.0.0",
    "prettier": "^2.8.4",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.9.5",
    "undici": "^5.21.0"
