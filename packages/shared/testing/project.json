{
  "root": "packages/shared/testing",
  "sourceRoot": "packages/shared/testing/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/shared/testing",
        "tsConfig": "packages/shared/testing/tsconfig.lib.json",
        "packageJson": "packages/shared/testing/package.json",
        "main": "packages/shared/testing/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/shared/testing/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/shared/testing"],
      "options": {
        "jestConfig": "packages/shared/testing/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:shared"]
}
