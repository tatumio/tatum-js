    "build:watch": "tsc -w",
    "prepublish": "npm run clean && npm run build",
    "lint": "eslint ./src --ext .ts --fix"
  },
  "dependencies": {
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.31.0",
    "@tatumio/tatum-core": "file:../tatum-core",
    "@tatumio/tatum-defi": "file:../tatum-defi",
    "@tatumio/tatum-ledger": "file:../tatum-ledger",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/validator": "^13.1.3",
    "eslint": "^7.32.0",
    "jest": "^27.2.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFiles": [
      "../../jest-setup-file.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
