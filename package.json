{
  "name": "@tatumcom/js",
  "version": "1.0.0",
  "description": "Tatum JS SDK",
  "author": "Tatum",
  "repository": "https://github.com/tatumio/tatum-js",
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "keywords": [
    "Tatum",
    "Blockchain"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "./dist/src/main.js",
  "types": "./dist/src/main.d.ts",
  "scripts": {
    "cli": "ts-node src/cli.ts",
